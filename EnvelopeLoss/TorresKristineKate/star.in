sName        star       # Body's name
saModules    stellar

# Physical Parameters
dMass        0.139      # Mass, negative -> Earth masses
dAge	     0.5e9

# STELLAR parameters
sStellarModel baraffe    # Stellar evolutin model: `baraffe` or `none`
dSatXUVFrac   1.e-3      # Saturation level of the XUV luminosity
dSatXUVTime   3.14e9
dXUVBeta      1.17
dRadGyra      0.5

saOutputOrder Time -Luminosity -LXUVTot -HZLimRunaway -HZlimMaxGreenhouse
