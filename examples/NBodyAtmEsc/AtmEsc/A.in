# Planet a parameters
sName                     A       # Body's name
saModules                 AtmEsc      # Modules to apply, exact spelling required

# Physical Properties
dMass                     -1.02        # Mass, negative -> Earth masses
#dRadius                   -0.383        # Radius, negative -> Earth radii
#dAge                      5e4
dEnvelopeMass             -1

# Orbital Properties
dSemi                     -0.18850      # Semi-major axis, negative -> AU
dEcc                       0.0          # Eccentricity


#AtmEsc Parameters
dXFrac                    1.0
dSurfWaterMass            0
bHaltEnvelopeGone         0
sPlanetRadiusModel        lopez
dAtmXAbsEffH              0.1
bUseEnergyLimited       1       # Is the flow energy-limited?

saOutputOrder             Time -EnvelopeMass -PlanetRadius -Mass
